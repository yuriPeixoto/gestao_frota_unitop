{
    "name": "unitopconsultoria/gestao_frota",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "chillerlan/php-qrcode": "^5.0",
        "consoletvs/charts": "^6.8",
        "guzzlehttp/guzzle": "^7.9",
        "intervention/image": "^2.7",
        "laravel/framework": "11.44.0",
        "laravel/reverb": "*",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "laraveldaily/laravel-charts": "^0.2.3",
        "lavela/phpjasper": "^3.0",
        "league/commonmark": "^2.6.0",
        "milon/barcode": "^12.0",
        "mkocansey/bladewind": "^2.8",
        "phpmailer/phpmailer": "^6.10",
        "phpoffice/phpspreadsheet": "^4.0",
        "pragmarx/google2fa": "^8.0",
        "predis/predis": "^3.2",
        "rap2hpoutre/laravel-log-viewer": "^2.5",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-permission": "^6.10",
        "ext-curl": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "fakerphp/faker": "^1.23",
        "kitloong/laravel-migrations-generator": "^7.0",
        "laravel-lang/attributes": "^2.13",
        "laravel-lang/lang": "^15.19",
        "laravel-lang/publisher": "^16.6",
        "laravel/breeze": "^2.2",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "laravel/telescope": "^5.11",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/ActivityHelper.php",
            "app/Helpers/DateFormatterHelper.php",
            "app/Helpers/CPFHelper.php",
            "app/Helpers/CNPJHelper.php",
            "app/Helpers/SanitizeToDouble.php",
            "app/Helpers/TimestampToDate.php",
            "app/Helpers/FormatDocs.php",
            "app/Helpers/PhoneHelper.php",
            "app/Helpers/GetterFilial.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
